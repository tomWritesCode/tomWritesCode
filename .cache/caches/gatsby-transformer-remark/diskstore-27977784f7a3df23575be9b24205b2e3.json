eger based on how long the array of jokes is. ","position":{"start":{"line":45,"column":1,"offset":2024},"end":{"line":45,"column":115,"offset":2138}}}],"position":{"start":{"line":45,"column":1,"offset":2024},"end":{"line":45,"column":115,"offset":2138}}},{"type":"text","value":"\n"},{"type":"element","tagName":"pre","properties":{},"children":[{"type":"element","tagName":"code","properties":{"className":["language-javascript"]},"children":[{"type":"text","value":"  function getRandomInt(max) {\n    return Math.floor(Math.random() * Math.floor(max))\n  }\n\n  function jokeGenerator() {\n    return JokeList[getRandomInt(JokeList.length)]\n  }\n"}],"position":{"start":{"line":47,"column":1,"offset":2140},"end":{"line":55,"column":6,"offset":2348}}}],"position":{"start":{"line":47,"column":1,"offset":2140},"end":{"line":55,"column":6,"offset":2348}}},{"type":"text","value":"\n"},{"type":"element","tagName":"hr","properties":{},"children":[],"position":{"start":{"line":57,"column":1,"offset":2350},"end":{"line":57,"column":4,"offset":2353}}},{"type":"text","value":"\n"},{"type":"element","tagName":"h2","properties":{},"children":[{"type":"text","value":"tomWritesCode - ","position":{"start":{"line":59,"column":4,"offset":2358},"end":{"line":59,"column":20,"offset":2374}}},{"type":"raw","value":"<a href=\"https://github.com/tomWritesCode/tomWritesCode\" target=\"_blank\">","position":{"start":{"line":59,"column":20,"offset":2374},"end":{"line":59,"column":93,"offset":2447}}},{"type":"text","value":"Check out the codebase","position":{"start":{"line":59,"column":93,"offset":2447},"end":{"line":59,"column":115,"offset":2469}}},{"type":"raw","value":"</a>","position":{"start":{"line":59,"column":115,"offset":2469},"end":{"line":59,"column":119,"offset":2473}}}],"position":{"start":{"line":59,"column":1,"offset":2355},"end":{"line":59,"column":119,"offset":2473}}},{"type":"text","value":"\n"},{"type":"element","tagName":"p","properties":{},"children":[{"type":"text","value"
n":43,"offset":363},"indent":[]}}],"position":{"start":{"line":8,"column":20,"offset":340},"end":{"line":8,"column":92,"offset":412},"indent":[]}}],"position":{"start":{"line":8,"column":1,"offset":321},"end":{"line":8,"column":92,"offset":412},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"  This site is built in Gatsby, but as well as React and GraphQL being packaged with Gatsby I am also using Styled Components as well as Emotion because I wanted to see how it worked. Last but not least, Contentful for holding all my blog posts. ","position":{"start":{"line":9,"column":1,"offset":413},"end":{"line":9,"column":247,"offset":659},"indent":[]}}],"position":{"start":{"line":9,"column":1,"offset":413},"end":{"line":9,"column":247,"offset":659},"indent":[]}},{"type":"code","lang":"javascript","value":"tomWritesCode = () => {\n  const React = true;\n  const GatsbyJS = true;\n  const Blog = Contentful;\n  const styling = StyledComponents;\n  const stylingAlso = Emotion;\n}","position":{"start":{"line":11,"column":1,"offset":661},"end":{"line":19,"column":6,"offset":863},"indent":[1,1,1,1,1,1,1,1]}},{"type":"thematicBreak","position":{"start":{"line":21,"column":1,"offset":865},"end":{"line":21,"column":4,"offset":868},"indent":[]}},{"type":"heading","depth":2,"children":[{"type":"text","value":"dadJokes() - ","position":{"start":{"line":22,"column":4,"offset":872},"end":{"line":22,"column":17,"offset":885},"indent":[]}},{"type":"link","title":null,"url":"https://github.com/tomWritesCode/dadJokes","children":[{"type":"text","value":"Check out the codebase","position":{"start":{"line":22,"column":18,"offset":886},"end":{"line":22,"column":40,"offset":908},"indent":[]}}],"position":{"start":{"line":22,"column":17,"offset":885},"end":{"line":22,"column":84,"offset":952},"indent":[]}}],"position":{"start":{"line":22,"column":1,"offset":869},"end":{"line":22,"column":84,"offset":952},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"  This doesn't need to exist, but here it is anyway. There was a running joke at the Code Heart Design Conference that we needed dad jokes in between the silence of people switching laptops. This was made the very next day. ","position":{"start":{"line":23,"column":1,"offset":953},"end":{"line":23,"column":225,"offset":1177},"indent":[]}}],"position":{"start":{"line":23,"column":1,"offset":953},"end":{"line":23,"column":225,"offset":1177},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"  Built in Gatsby because that is what I always reach for when I want to make something quickly. ","position":{"start":{"line":25,"column":1,"offset":1179},"end":{"line":25,"column":98,"offset":1276},"indent":[]}}],"position":{"start":{"line":25,"column":1,"offset":1179},"end":{"line":25,"column":98,"offset":1276},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"  The main logic behind it working is the array of objects in JokeList.js:","position":{"start":{"line":27,"column":1,"offset":1278},"end":{"line":27,"column":75,"offset":1352},"indent":[]}}],"position":{"start":{"line":27,"column":1,"offset":1278},"end":{"line":27,"column":75,"offset":1352},"indent":[]}},{"type":"code","lang":"javascript","value":"  const JokeList = [\n    {\n      joke: `Did you hear about the restaurant on the moon?`,\n      punchLine: `Great food, no atmosphere.`,\n    },\n  ]","position":{"start":{"line":29,"column":1,"offset":1354},"end":{"line":36,"column":6,"offset":1534},"indent":[1,1,1,1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"  This is then used by the JokeGenerator.js file when someone presses the 'Just one more.' button which triggers the NewJoke() function:","position":{"start":{"line":37,"column":1,"offset":1535},"end":{"line":37,"column":137,"offset":1671},"indent":[]}}],"position":{"start":{"line":37,"column":1,"offset":1535},"end":{"line":37,"column":137,"offset":1671},"indent":[]}},{"type":"code","lang":"javascript","value":"function NewJoke() {\n// jokeGenerator();\n  const returnedJoke = jokeGenerator()\n// Will not generate joke twice in a row\n  document.getElementById('Joke').innerHTML = returnedJoke.joke\n  document.getElementById('Punchline').innerHTML = returnedJoke.punchLine\n}","position":{"start":{"line":39,"column":1,"offset":1673},"end":{"line":47,"column":6,"offset":1969},"indent":[1,1,1,1,1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"  The JokeGenerator function runs first which generates a random Integer based on how long the array of jokes is. ","position":{"start":{"line":49,"column":1,"offset":1971},"end":{"line":49,"column":115,"offset":2085},"indent":[]}}],"position":{"start":{"line":49,"column":1,"offset":1971},"end":{"line":49,"column":115,"offset":2085},"indent":[]}},{"type":"code","lang":"javascript","value":"  function getRandomInt(max) {\n    return Math.floor(Math.random() * Math.floor(max))\n  }\n\n  function jokeGenerator() {\n    return JokeList[getRandomInt(JokeList.length)]\n  }","position":{"start":{"line":51,"column":1,"offset":2087},"end":{"line":59,"column":6,"offset":2295},"indent":[1,1,1,1,1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"  Once that has been done the selected joke is then set into the joke and punchline sections of the page. ","position":{"start":{"line":60,"column":1,"offset":2296},"end":{"line":60,"column":107,"offset":2402},"indent":[]}}],"position":{"start":{"line":60,"column":1,"offset":2296},"end":{"line":60,"column":107,"offset":2402},"indent":[]}}],"position":{"start":{"line":1,"column":1,"offset":0},"end":{"line":61,"column":1,"offset":2403}}}}
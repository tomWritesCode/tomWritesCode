\n    },\n  ]","position":{"start":{"line":23,"column":1,"offset":1339},"end":{"line":30,"column":6,"offset":1519},"indent":[1,1,1,1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"  This is then used by the JokeGenerator.js file when someone presses the 'Just one more.' button which triggers the NewJoke() function:","position":{"start":{"line":31,"column":1,"offset":1520},"end":{"line":31,"column":137,"offset":1656},"indent":[]}}],"position":{"start":{"line":31,"column":1,"offset":1520},"end":{"line":31,"column":137,"offset":1656},"indent":[]}},{"type":"code","lang":"javascript","value":"function NewJoke() {\n// jokeGenerator();\n  const returnedJoke = jokeGenerator()\n// Will not generate joke twice in a row\n  document.getElementById('Joke').innerHTML = returnedJoke.joke\n  document.getElementById('Punchline').innerHTML = returnedJoke.punchLine\n}","position":{"start":{"line":33,"column":1,"offset":1658},"end":{"line":41,"column":6,"offset":1954},"indent":[1,1,1,1,1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"  The JokeGenerator function runs first which generates a random Integer based on how long the array of jokes is. ","position":{"start":{"line":43,"column":1,"offset":1956},"end":{"line":43,"column":115,"offset":2070},"indent":[]}}],"position":{"start":{"line":43,"column":1,"offset":1956},"end":{"line":43,"column":115,"offset":2070},"indent":[]}},{"type":"code","lang":"javascript","value":"  function getRandomInt(max) {\n    return Math.floor(Math.random() * Math.floor(max))\n  }\n\n  function jokeGenerator() {\n    return JokeList[getRandomInt(JokeList.length)]\n  }","position":{"start":{"line":45,"column":1,"offset":2072},"end":{"line":53,"column":6,"offset":2280},"indent":[1,1,1,1,1,1,1,1]}},{"type":"thematicBreak","position":{"start":{"line":55,"column":1,"offset":2282},"end":{"line":55,"column":4,"offset":2285},"indent":[]}},{"type":"heading","depth":2,"children":[{"type":"text","value":"tomWritesCode - ","position"
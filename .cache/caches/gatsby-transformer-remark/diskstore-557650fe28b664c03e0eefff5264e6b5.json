mn":176,"offset":1027},"end":{"line":24,"column":234,"offset":1085},"indent":[]}}],"position":{"start":{"line":24,"column":1,"offset":852},"end":{"line":24,"column":234,"offset":1085},"indent":[]}},{"type":"thematicBreak","position":{"start":{"line":26,"column":1,"offset":1087},"end":{"line":26,"column":4,"offset":1090},"indent":[]}},{"type":"heading","depth":2,"children":[{"type":"text","value":"dadJokes() - ","position":{"start":{"line":27,"column":4,"offset":1094},"end":{"line":27,"column":17,"offset":1107},"indent":[]}},{"type":"link","title":null,"url":"https://github.com/tomWritesCode/dadJokes","children":[{"type":"text","value":"Check out the codebase","position":{"start":{"line":27,"column":18,"offset":1108},"end":{"line":27,"column":40,"offset":1130},"indent":[]}}],"position":{"start":{"line":27,"column":17,"offset":1107},"end":{"line":27,"column":84,"offset":1174},"indent":[]}}],"position":{"start":{"line":27,"column":1,"offset":1091},"end":{"line":27,"column":84,"offset":1174},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"  This doesn't need to exist, but here it is anyway. There was a running joke at the Code Heart Design Conference that we needed dad jokes in between the silence of people switching laptops. This was made the very next day. ","position":{"start":{"line":28,"column":1,"offset":1175},"end":{"line":28,"column":225,"offset":1399},"indent":[]}}],"position":{"start":{"line":28,"column":1,"offset":1175},"end":{"line":28,"column":225,"offset":1399},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"  Built in Gatsby because that is what I always reach for when I want to make something quickly. ","position":{"start":{"line":30,"column":1,"offset":1401},"end":{"line":30,"column":98,"offset":1498},"indent":[]}}],"position":{"start":{"line":30,"column":1,"offset":1401},"end":{"line":30,"column":98,"offset":1498},"indent":[]}},{"type":"paragraph","children":[{"type":"text","value":"  The main logic behind it working is the array of objects in JokeList.js:","position":{"start":{"line":32,"column":1,"offset":1500},"end":{"line":32,"column":75,"offset":1574},"indent":[]}}],"position":{"start":{"line":32,"column":1,"offset":1500},"end":{"line":32,"column":75,"offset":1574},"indent":[]}},{"type":"code","lang":"javascript","value":"  const JokeList = [\n    {\n      joke: `Did you hear about the restaurant on the moon?`,\n      punchLine: `Great food, no atmosphere.`,\n    },\n  ]","position":{"start":{"line":34,"column":1,"offset":1576},"end":{"line":41,"column":6,"offset":1756},"indent":[1,1,1,1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"  This is then used by the JokeGenerator.js file when someone presses the 'Just one more.' button which triggers the NewJoke() function:","position":{"start":{"line":42,"column":1,"offset":1757},"end":{"line":42,"column":137,"offset":1893},"indent":[]}}],"position":{"start":{"line":42,"column":1,"offset":1757},"end":{"line":42,"column":137,"offset":1893},"indent":[]}},{"type":"code","lang":"javascript","value":"function NewJoke() {\n// jokeGenerator();\n  const returnedJoke = jokeGenerator()\n// Will not generate joke twice in a row\n  document.getElementById('Joke').innerHTML = returnedJoke.joke\n  document.getElementById('Punchline').innerHTML = returnedJoke.punchLine\n}","position":{"start":{"line":44,"column":1,"offset":1895},"end":{"line":52,"column":6,"offset":2191},"indent":[1,1,1,1,1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"  The JokeGenerator function runs first which generates a random Integer based on how long the array of jokes is. ","position":{"start":{"line":54,"column":1,"offset":2193},"end":{"line":54,"column":115,"offset":2307},"indent":[]}}],"position":{"start":{"line":54,"column":1,"offset":2193},"end":{"line":54,"column":115,"offset":2307},"indent":[]}},{"type":"code","lang":"javascript","value":"  function getRandomInt(max) {\n    return Math.floor(Math.random() * Math.floor(max))\n  }\n\n  function jokeGenerator() {\n    return JokeList[getRandomInt(JokeList.length)]\n  }","position":{"start":{"line":56,"column":1,"offset":2309},"end":{"line":64,"column":6,"offset":2517},"indent":[1,1,1,1,1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"  Once that has been done the selected joke is then set into the joke and punchline sections of the page. ","position":{"start":{"line":65,"column":1,"offset":2518},"end":{"line":65,"column":107,"offset":2624},"indent":[]}}],"position":{"start":{"line":65,"column":1,"offset":2518},"end":{"line":65,"column":107,"offset":2624},"indent":[]}}],"position":{"start":{"line":1,"column":1,"offset":0},"end":{"line":66,"column":1,"offset":2625}}}}